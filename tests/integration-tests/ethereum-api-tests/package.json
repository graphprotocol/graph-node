  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.69.0",
    "@graphprotocol/graph-ts": "0.36.0-alpha-20240422133139-8761ea3",
    "solc": "^0.8.2"
  },
  "dependencies": {
    "@truffle/contract": "^4.3",
    "@truffle/hdwallet-provider": "^1.2",
    "apollo-fetch": "^0.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "gluegun": "^4.6.1",
    "truffle": "^5.2"
  }
}
